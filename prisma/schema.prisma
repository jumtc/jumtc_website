generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Images {
  id       Int    @id @default(autoincrement())
  publicid String @unique @db.VarChar(255)
  format   String @db.VarChar(255)
  version  String @db.VarChar(255)
}
